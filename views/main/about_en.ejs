        </div>
        <div class="row mt-5 text-center">
          <%
          if(list_target.length!=0){
          var i=1;
          list_target.slice(0,3).forEach(function(target){
          %>
          <div class="col-lg-4 py-3 wow fadeInUp">
            <div class="display-3"><i class="<%= target.icon %>" style="color: #142940;"></i> </div>
            <h5 style="font-weight: bold;"><%= target.target %></h5>
            <p><%= target.deskripsi %></p>
            <!-- <p>Jumlah rincian target <Span><%= target.total_rincian %></Span></p> -->
          </div>
          <%  i++; }) %>
          <% } else { %>
          <div class="col-lg-4 py-3 wow fadeInUp">
            <div class="display-3"><i class="<%= target.icon %>" style="color: #142940;"></i> </div>
            <h5 style="font-weight: bold;">Belum ada target</h5>
            <p>Belum ada target</p>
          </div>
          <% } %>
        </div>
        <div class="text-center">
          <a href="/main/detail_target_pph_en/<%= region[0].id_region %>" class="btn ent"><span style="margin:0 15px">See More Target </span><i class="fas fa-caret-right"
              style="font-size: 18px;vertical-align: middle;float: right;margin-top: 2px;"></i></a>
        </div>
      </div> <!-- .container -->
    </div> <!-- .page-section -->

 
